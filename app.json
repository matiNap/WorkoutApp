{
  "expo": {
    "name": "Workout Timer - porfolio app",
    "slug": "workout_timer_porfolio_app",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2e2e2e"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#00000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "package": "napieralski.mateusz.workouttimer",
      "versionCode": 1
    },
    "ios": {
      "supportsTablet": false,
      "infoPlist": {
        "UIBackgroundModes": ["audio"]
      }
    },
    "description": "Workout timer applications as my portfolio.",
    "githubUrl": "https://github.com/matiNap/WorkoutApp"
  }
}
